<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
        <key>Label</key>
        <string>dotfiles.backup</string>
        <key>ProgramArguments</key>
        <array>
                <string>/Users/USERNAME/.dotfiles/cronjobs/backup.sh</string>
        </array>
        <key>StartCalendarInterval</key>
        <dict>
                <key>Hour</key>
                <integer>12</integer>
                <key>Minute</key>
                <integer>00</integer>
        </dict>

        <key>StandardOutPath</key>
        <string>/Users/USERNAME/Library/Logs/dotfiles-backup.log</string>

        <key>StandardErrorPath</key>
        <string>/Users/USERNAME/Library/Logs/dotfiles-backup-errors.log</string>
</dict>
</plist>